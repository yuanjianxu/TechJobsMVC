<h2>@ViewBag.title</h2>
<h3>@ViewBag.items.Count Result(s)</h3>

<table>
@for (int i= 0; i < ViewBag.items.Count; i++)
{
    <tr class="@( i % 2 == 0 ? "even" : "odd" ) ">
        <td><a asp-controller="List" asp-action="Jobs" asp-route-column="@ViewBag.column" asp-route-value="@ViewBag.items[i]">@ViewBag.items[i]</a></td>
    </tr>  
}   
</table>
